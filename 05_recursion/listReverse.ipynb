    "def reverse_recursive(lst):\n",
    "    '''reversed(lst) = reversed(lst[1::]) + lst[0]'''\n",
    "    if len(lst) <= 1:\n",
    "        return lst\n",
    "    return reverse_recursive(lst[1::]) + [lst[0]]\n",
    "\n",
    "\n",
    "testlist = [1, 2, 3, 4, 5] # [5, 4, 3, 2, 1]\n",
    "print(reverse_recursive(testlist))"
   ]
  }
